.header.clearfix
  %h3 September 4th 2011 - April 1st 2013(test)
  %p.info Commits to master, excluding merge commits

.graphs
  #contributors-master
  #contributors.clearfix
    %ol.contributors-list.clearfix

:javascript
  Graphs.draw_total_commits(#{@display});
  var authors = _.sortBy(#{@by_author}, function(d) { return d.total }).reverse();
  $.each(authors, function (key, value) {
      $(".contributors-list").append("<li class='person' style='display: block;'><h4>" + this.author + " (" +  this.total + " commits)</h4></li>");
    Graphs.draw_contributors_commits(_.omit(value,['author', 'total']));
  });